
# Build everything in this folder
all: numPrefix16 numPrefix32 numPrefix8

# numPrefix tools
numPrefix16: numPrefix16.c
	cc -o numPrefix/numPrefix16 numPrefix/numPrefix16.c

numPrefix32: numPrefix32.c
	cc -o numPrefix/numPrefix32 numPrefix/numPrefix32.c

numPrefix8: numPrefix8.c
	cc -o numPrefix/numPrefix8 numPrefix/numPrefix8.c
